{
    "name": "supseven/supi",
    "description": "Cookie Consent popover; easy, yet powerful",
    "license": "GPL-3.0-or-later",
    "type": "typo3-cms-extension",
    "config": {
        "bin-dir": "bin/",
        "platform": {
            "php": "7.2.6"
        },
        "gitlab-domains": ["gitlab.sup7.at"]
    },
    "autoload": {
        "psr-4": {
            "Supseven\\Supi\\": "Classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Supseven\\Supi\\Tests\\": "Tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "friendsofphp/php-cs-fixer": "*"
    },
    "extra": {
        "typo3/cms": {
            "web-dir": "vendor/_www/"
        },
        "installer-name": "supi"
    },
    "require": {
        "typo3/cms-fluid": "^10.1",
        "typo3/cms-frontend": "^10.1"
    }
}
